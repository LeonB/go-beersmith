<F_M_UNITS>5</F_M_UNITS>
<F_M_AMOUNT>100.0000000</F_M_AMOUNT>
<F_M_VOLUME>768.0000000</F_M_VOLUME>
<F_M_INVENTORY>0.0000000</F_M_INVENTORY>
